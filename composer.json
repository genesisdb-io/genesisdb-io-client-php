{
  "name": "genesisdb/client-sdk",
  "description": "PHP SDK for Genesis DB",
  "type": "library",
  "require": {
    "php": ">=8.1",
    "guzzlehttp/guzzle": "^7.0",
    "cloudevents/sdk-php": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "GenesisDB\\GenesisDB\\": "src/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
